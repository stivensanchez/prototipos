<p id="notice"><%= notice %></p>

    <h1>Listing Jodas</h1>
    
    <table class="table table-hover">
      <thead>
        <tr>
          <th>Nombre</th>
          <th>Apellido</th>
          <th>Telefono</th>
          <th colspan="3"></th>
        </tr>
      </thead>
    
      <tbody>
        <% @jodas.each do |joda| %>
          <tr>
            <td><%= joda.nombre %></td>
            <td><%= joda.apellido %></td>
            <td><%= joda.telefono %></td>
            <td><div class="links"><%= link_to 'Show', joda %></div></td>
            <td><div class="links"><%= link_to 'Edit', edit_joda_path(joda) %></div></td>
            <td><div class="links"><%= link_to 'Destroy', joda, method: :delete, data: { confirm: 'Are you sure?' } %></div></td>
          </tr>
        <% end %>
      </tbody>
    </table>
    
    <br>
    
    
    <div class="links">
      <%= link_to 'New Joda', new_joda_path %>
    </div>
    
    <div class="links">
      <div class="linkex">
        <a href=javascript:window.print();>Imprimir esta p√°gina</a>
        <%= link_to image_tag("impresora.png", :size => "30x30"), class: 'btn btn-default' %>
      </div>
    </div>


